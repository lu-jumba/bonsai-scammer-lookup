{
  "name": "@mailchain/api",
  "version": "0.22.0",
  "description": "Mailchain api tools",
  "license": "Apache-2.0",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "api"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "dependencies": {
    "@mailchain/crypto": "0.22.0",
    "@mailchain/encoding": "0.22.0",
    "axios": "1.3.4",
    "lodash": "^4.17.21"
  },
  "types": "./types/index.d.ts",
  "browser": "./browser/index.js",
  "main": "./node/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./browser/index.js",
      "require": "./node/index.js"
    },
    "./api": {
      "types": "./types/api/index.d.ts",
      "import": "./browser/api/index.js",
      "require": "./node/api/index.js"
    },
    "./api/api": {
      "types": "./types/api/api.d.ts",
      "import": "./browser/api/api.js",
      "require": "./node/api/api.js"
    },
    "./api/base": {
      "types": "./types/api/base.d.ts",
      "import": "./browser/api/base.js",
      "require": "./node/api/base.js"
    },
    "./api/common": {
      "types": "./types/api/common.d.ts",
      "import": "./browser/api/common.js",
      "require": "./node/api/common.js"
    },
    "./api/configuration": {
      "types": "./types/api/configuration.d.ts",
      "import": "./browser/api/configuration.js",
      "require": "./node/api/configuration.js"
    },
    "./helpers/apiKeyToCryptoKey": {
      "types": "./types/helpers/apiKeyToCryptoKey.d.ts",
      "import": "./browser/helpers/apiKeyToCryptoKey.js",
      "require": "./node/helpers/apiKeyToCryptoKey.js"
    },
    "./helpers/cryptoKeyToApiKey": {
      "types": "./types/helpers/cryptoKeyToApiKey.d.ts",
      "import": "./browser/helpers/cryptoKeyToApiKey.js",
      "require": "./node/helpers/cryptoKeyToApiKey.js"
    },
    "./helpers/errors": {
      "types": "./types/helpers/errors.d.ts",
      "import": "./browser/helpers/errors.js",
      "require": "./node/helpers/errors.js"
    }
  }
}
