{
  "name": "@mailchain/message-composer",
  "version": "0.22.0",
  "description": "Mailchain MIME message composer. RFC-5322 compliant",
  "license": "Apache-2.0",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "message-composer"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "dependencies": {
    "@mailchain/encoding": "0.22.0",
    "date-fns": "^2.29.2"
  },
  "types": "./types/index.d.ts",
  "browser": "./browser/index.js",
  "main": "./node/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./browser/index.js",
      "require": "./node/index.js"
    },
    "./consts": {
      "types": "./types/consts.d.ts",
      "import": "./browser/consts.js",
      "require": "./node/consts.js"
    },
    "./contentHandler": {
      "types": "./types/contentHandler.d.ts",
      "import": "./browser/contentHandler.js",
      "require": "./node/contentHandler.js"
    },
    "./fallback": {
      "types": "./types/fallback.d.ts",
      "import": "./browser/fallback.js",
      "require": "./node/fallback.js"
    },
    "./folding": {
      "types": "./types/folding.d.ts",
      "import": "./browser/folding.js",
      "require": "./node/folding.js"
    },
    "./hasOnlyAscii": {
      "types": "./types/hasOnlyAscii.d.ts",
      "import": "./browser/hasOnlyAscii.js",
      "require": "./node/hasOnlyAscii.js"
    },
    "./headerFactories": {
      "types": "./types/headerFactories.d.ts",
      "import": "./browser/headerFactories.js",
      "require": "./node/headerFactories.js"
    },
    "./headerHandler": {
      "types": "./types/headerHandler.d.ts",
      "import": "./browser/headerHandler.js",
      "require": "./node/headerHandler.js"
    },
    "./headerOrder": {
      "types": "./types/headerOrder.d.ts",
      "import": "./browser/headerOrder.js",
      "require": "./node/headerOrder.js"
    },
    "./messageComposer": {
      "types": "./types/messageComposer.d.ts",
      "import": "./browser/messageComposer.js",
      "require": "./node/messageComposer.js"
    },
    "./messageComposerContext": {
      "types": "./types/messageComposerContext.d.ts",
      "import": "./browser/messageComposerContext.js",
      "require": "./node/messageComposerContext.js"
    },
    "./types": {
      "types": "./types/types.d.ts",
      "import": "./browser/types.js",
      "require": "./node/types.js"
    }
  }
}
