{
  "name": "@mailchain/sdk",
  "version": "0.22.0",
  "description": "Mailchain sdk for sending messages to web3 addresses",
  "license": "Apache-2.0",
  "author": {
    "name": "Mailchain",
    "email": "support@mailchain.co",
    "url": "https://mailchain.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mailchain/mailchain-sdk-js.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/mailchain/mailchain-sdk-js/issues"
  },
  "homepage": "https://docs.mailchain.com/developer",
  "dependencies": {
    "@mailchain/crypto": "0.22.0",
    "@mailchain/encoding": "0.22.0",
    "@mailchain/internal": "0.22.0",
    "@mailchain/keyring": "0.22.0"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "typesVersions": {
    "*": {
      "internal": [
        "./types/internal/index.d.ts"
      ]
    }
  },
  "types": "./types/index.d.ts",
  "browser": "./browser/index.js",
  "main": "./node/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./browser/index.js",
      "require": "./node/index.js"
    },
    "./internal": {
      "types": "./types/internal/index.d.ts",
      "import": "./browser/internal/index.js",
      "require": "./node/internal/index.js"
    }
  }
}
